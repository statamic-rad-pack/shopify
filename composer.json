{
  "name": "statamic-rad-pack/statamic-shopify",
  "description": "A Shopify Addon for Statamic",
  "license": "proprietary",
  "autoload": {
    "psr-4": {
      "StatamicRadPack\\Shopify\\": "src"
    }
  },
  "authors": [
    {
      "name": "Jack Whiting",
      "email": "hi@jackwhiting.co.uk",
      "homepage": "https://jackwhiting.co.uk",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "hi@jackwhiting.co.uk"
  },
  "extra": {
    "statamic": {
      "name": "Shopify",
      "description": "Shopify Addon for Statamic"
    },
    "laravel": {
      "providers": [
        "StatamicRadPack\\Shopify\\ServiceProvider"
      ]
    }
  },
  "require": {
    "phpclassic/php-shopify": "^1.1",
    "statamic/cms": "^4.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
